{
  "name": "@jenkins-cd/preferences",
  "version": "0.0.5",
  "description": "Jenkins BlueOcean preference storage via localStorage",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "jasmine spec/*.js",
    "jsdoc": "jsdoc -d jsdoc preferences/*.js JSDOC.md && mkdir -p jsdoc/images && cp images/* jsdoc/images",
    "prepublish": "babel preferences --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jenkinsci/js-preferences.git"
  },
  "keywords": [
    "localStorage",
    "config",
    "preferences"
  ],
  "author": "Thorsten Scherler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scherler/js-preferences/issues"
  },
  "homepage": "https://github.com/scherler/js-preferences#readme",
  "dependencies": {
    "@jenkins-cd/storage": "0.0.6"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "jasmine": "2.5.3",
    "jsdoc": "3.4.3",
    "jsdom": "10.1.0"
  }
}
